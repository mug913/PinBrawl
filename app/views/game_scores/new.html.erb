<html>
    <body>
    
    <%= form_for(@score) do |f| %>
    
        <%= f.label :score %>
        <%= f.number_field :score %> 
        <%= f.label :score_date %>
        <%= f.date_field :score_date %>

        <%= f.label :game_table, "Select a table" %>
        <%= f.collection_select :game_table_id, GameTable.all, :id, :name %>

        <%= f.submit "Record Score" %>
    <% end %>

    <br>
    
    <%= link_to 'New Table', new_game_table_path%>
    <br>


    </body>
</html>
    