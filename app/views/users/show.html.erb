<h1> <%= @user.username%>'s Score Log: </h1>
<html>
<body>

    <% @user.game_scores.each do |s|%>
    <%=s.score%> 
    <%=s.game_table.name%>
    <br>
    <%end%>
    
    <br>
    Number of Scores: <%= @user.game_scores.count%>
    <br>
    
    <%= link_to 'Record New Score', new_user_game_score_path(@user)%>
    <br>
    <br>
    <%= link_to 'All Scores', user_game_scores_path(@user)%>
    <br>
        <%= link_to 'Log out', logout_path, method: :delete %>
    </body>
</html>